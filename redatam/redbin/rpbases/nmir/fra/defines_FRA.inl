[DEFINE14]
NAME=AGENUMER
ENTITY=%OUTENT
AS=COUNT
EXPRESSION=PERSON
FOR=( PERSON.AGEREC=1 OR PERSON.AGEREC=3 ) AND %FOREXPR
TYPE=INTEGER
DEPENDENCIES=1
DEPENDENCY1=PERSON.AGEREC

[DEFINE15]
NAME=AGEDENOM
ENTITY=%OUTENT
AS=COUNT
EXPRESSION=PERSON
FOR=PERSON.AGEREC = 2 AND %FOREXPR
TYPE=INTEGER
DEPENDENCIES=1
DEPENDENCY1=PERSON.AGEREC

[DEFINE16]
NAME=INDEPR
CAPTION=Relation de Dépendance
ENTITY=%OUTENT
AS=EXPRESSION
EXPRESSION=( %OUTENT.AGENUMER / %OUTENT.AGEDENOM )
FOR=%OUTENT.AGEDENOM > 0
TYPE=REAL
DECIMALS=2
DEPENDENCIES=2
DEPENDENCY1=%OUTENT.AGENUMER
DEPENDENCY2=%OUTENT.AGEDENOM

