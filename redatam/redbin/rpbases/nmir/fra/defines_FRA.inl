[DEFINE18]
NAME=INMDENOC
ENTITY=%OUTENT
AS=COUNT
EXPRESSION=PERSON
FOR=( PERSON.AGE>=15 AND PERSON.AGE<=49 AND PERSON.SEXE=2 ) AND %FOREXPR
TYPE=INTEGER

[DEFINE19]
NAME=INDMC
CAPTION=Quotient Enfants<5 age / Femmes
ENTITY=%OUTENT
